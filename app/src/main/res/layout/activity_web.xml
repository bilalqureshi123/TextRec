<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.raheelayasmin.textrec.WebActivity">

    <WebView
        android:id="@+id/webView1"
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp" >

    </WebView>

    <Button
        android:id="@+id/myBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:onClick="goBack"


        android:text="BACK"
        app:layout_constraintBottom_toTopOf="@+id/webView1"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintLeft_toRightOf="@+id/webView1"
        app:layout_constraintRight_toLeftOf="@+id/webView1"
        app:layout_constraintTop_toBottomOf="@+id/webView1"
        app:layout_constraintVertical_bias="0.103" />
</android.support.constraint.ConstraintLayout>
